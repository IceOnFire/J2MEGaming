package it.seat.helicoptergame.states;

import it.seat.game.states.State;

public class OutOfViewportState implements State {
	public final static int LEFT = 0x04;// GameCanvas.LEFT_PRESSED;
	public final static int RIGHT = 0x20;// GameCanvas.RIGHT_PRESSED;
	public final static int TOP = 0x2;// GameCanvas.UP_PRESSED;
	public final static int BOTTOM = 0x40;// GameCanvas.DOWN_PRESSED;

	private int bounds;

	public int getBounds() {
		return bounds;
	}

	public void setBounds(int bounds) {
		this.bounds = bounds;
	}

}
